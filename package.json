{
  "name": "component-library",
  "version": "1.1.9",
  "description": "An easy way to create HTML styleguide for your project.",
  "author": "Dima Snisarenko <snisarenkodima@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sneas/component-library.git"
  },
  "keywords": [
    "ui",
    "component-library",
    "styleguide"
  ],
  "main": "dist/index.js",
  "scripts": {
    "start": "ws -d ./demo/public -c",
    "prebuild": "rimraf dist",
    "build": "babel --out-dir dist --ignore *.test.js src --copy-files",
    "precompile": "npm run build",
    "compile": "gulp --gulpfile demo/gulpfile.js compile",
    "compile:watch": "watch 'npm run compile' ./src"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "directory-tree": "^1.0.0",
    "font-awesome": "^4.6.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "lodash": "^4.13.1",
    "node-modules-resolve": "^1.3.0",
    "npm-sass": "^1.3.0",
    "nunjucks": "3.0.0",
    "writefile": "^0.2.8"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "local-web-server": "^1.2.6",
    "rimraf": "2.5.4",
    "watch": "1.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
